pattern: 
 - "(.+)\\.drl"
afterPackage:
 - "import function org.drools.examples.helloworldaop.UtilityClass.trace"
 - "import function org.drools.examples.helloworldaop.UtilityClass.trace2"
afterRule:
 - "salience 5"
afterWhen:
 - "eval( trace( \"after when\" ) )"
beforeThen:
 - "eval( trace( \"before when\" ) )"
afterThen:
 - "trace( \"after then\" );"
beforeEnd:
 - "trace( \"before end\" );"